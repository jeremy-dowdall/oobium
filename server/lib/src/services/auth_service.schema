User
  name String
  avatar String
  token Token
  referredBy User

Token
  user User

Link // used to link external accounts (like Firebase Auth) to a User
  user User(required)
  type String(required)
  code String(required) // eg firebase auth id
  data Map<String, String> // extra data from the linked account, stuff like name, email, photoUrl, etc

Group
  name String
  owner User

Membership
  user User
  group Group
  invitedBy User
